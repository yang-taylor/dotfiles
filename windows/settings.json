{
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "Iosevka",
    "editor.fontSize": 18,
	"editor.suggestSelection": "first",
    "git.autofetch": true,
    "editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"telemetry.enableCrashReporter": false,
	"org.addLeftZero": true,
	"org.todoKeywords": [
		"APPT",
		"IN-PROGRESS",
		"TODO",
		"ONGOING",
		"DUE",
		"DONE"
	],
	"editor.copyWithSyntaxHighlighting": false,
	"editor.defaultFormatter": "formulahendry.code-runner",
	"editor.minimap.enabled": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.preferredDarkColorTheme": "Horizon",
	"workbench.preferredHighContrastColorTheme": "Monokai Dimmed",
	"workbench.preferredLightColorTheme": "Kimbie Dark",
	"workbench.iconTheme": "Monokai Pro Icons",
	"workbench.tips.enabled": false,
	"editor.wordWrap": "bounded",
	"terminal.explorerKind": "external",
	"telemetry.enableTelemetry": false,
	"vim.smartRelativeLine": true,
	"[handlebars]": {
		"editor.suggest.insertMode": "replace"
	},
	"vscode-pets.petSize": "medium",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"workbench.panel.defaultLocation": "right",
	"workbench.editorAssociations": [
		{
			"viewType": "jupyter.notebook.ipynb",
			"filenamePattern": "*.ipynb"
		}
	],
	"latex.server.autoDownload": true,
	"workbench.colorTheme": "Horizon",
}